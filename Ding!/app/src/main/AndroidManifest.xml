<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.goky.ding" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameScreen"
            android:label="@string/title_activity_game_screen" >
        </activity>
        <activity
            android:name=".CircularAnimation"
            android:label="@string/title_activity_circular_animation" >
        </activity>
        <activity
            android:name=".LoseScreen"
            android:label="@string/title_activity_lose_screen" >
        </activity>
    </application>

</manifest>
